{
  "name": "learning-angular2",
  "version": "1.0.0",
  "description": "learning angular2",
  "main": "index.html",
  "scripts": {
    "postinstall": "npm run typings-install",
    "typings-install": "typings install",
    "prebuild": "rm -rf dist",
    "build": "compass compile && webpack --progress --display-error-details --display-cached",
    "watch": "concurrently \"compass watch\" \"webpack --progress --display-error-details --display-cached --watch\"",
    "dev": "concurrently \"compass watch\" \"webpack-dev-server --inline --colors --progress --hot\"",
    "start": "npm run dev",
    "clean": "rm -rf node_modules typings dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/learning-angular2.git"
  },
  "keywords": [
    "angular2",
    "typescript"
  ],
  "author": "beginor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beginor/learning-angular2/issues"
  },
  "homepage": "https://github.com/beginor/learning-angular2#readme",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.5",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23",
    "bootstrap": "4.0.0-alpha.4",
    "font-awesome": "^4.6.3"
  },
  "devDependencies": {
    "@angular/compiler-cli": "0.6.2",
    "@angular/platform-server": "2.0.0",
    "angular2-template-loader": "^0.5.0",
    "concurrently": "^2.2.0",
    "css-loader": "^0.25.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1"
  }
}
