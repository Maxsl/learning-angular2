{
  "name": "learning-angular2",
  "version": "1.0.0",
  "description": "learning angular2",
  "main": "index.html",
  "scripts": {
    "postinstall": "npm run typings-install",
    "typings-install": "typings install",
    "dev": "concurrently \"compass watch\" \"gulp dev\"",
    "prebuild": "rm -rf dist",
    "build": "compass compile && gulp build",
    "start": "npm run dev",
    "prestart": "npm run build",
    "clean": "rm -rf node_modules typings dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/learning-angular2.git"
  },
  "keywords": [
    "angular2",
    "typescript"
  ],
  "author": "beginor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beginor/learning-angular2/issues"
  },
  "homepage": "https://github.com/beginor/learning-angular2#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.0",
    "bootstrap": "^4.0.0-alpha.3",
    "core-js": "^2.4.0",
    "font-awesome": "^4.6.3",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-live-server": "0.0.29",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.13.3",
    "typescript": "^1.8.10"
  }
}
